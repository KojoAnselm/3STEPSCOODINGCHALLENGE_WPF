<Window x:Class="Membership.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Height="760" Width="1250" WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow"
        WindowState="Maximized"
        MinWidth="1024" MinHeight="700">
    
    <ContentControl Name="ShowContent" HorizontalAlignment="Center" Content="{Binding ActiveWindow,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>


</Window>
